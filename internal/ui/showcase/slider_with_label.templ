package showcase

import "github.com/gijan/templui/pkg/components"

templ SliderWithLabel() {
	<div class="w-full max-w-sm">
		@components.Slider(components.SliderProps{
			ID:    "with-label",
			Name:  "with-label",
			Label: "Volume",
			Value: 65,
			Min:   0,
			Max:   100,
			Step:  1,
		})
	</div>
}
