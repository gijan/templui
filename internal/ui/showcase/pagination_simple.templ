package showcase

import (
	"github.com/axzilla/templui/components"
)

templ PaginationSimple() {
	@components.Pagination(components.PaginationProps{
		CurrentPage:  2,
		TotalPages:   5,
		Type:         components.PaginationTypeSimple,
		ShowNumbers:  false,
		ShowControls: true,
		// UrlPattern:   "/products?page={page}",
	})
}
